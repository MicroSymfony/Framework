{
    "name": "microsymfony/framework",
    "description": "A bundle for symfony designated to ease life when creating micro services",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrius Petrauskis",
            "email": "andrius@petrauskis.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "MicroSymfony\\Framework\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MicroSymfony\\Framework\\Test\\": "tests/"
        }
    },
    "require": {
        "symfony/http-kernel": "^4.0",
        "php": "^7.1",
        "microsymfony/services-connector": "^1.0",
        "microsymfony/jwt-auth": "^1.1",
        "symfony/dependency-injection": "^4.0",
        "symfony/config": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    }
}
